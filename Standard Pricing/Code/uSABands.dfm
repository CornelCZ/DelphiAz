  object qryPBN: TADOQuery
    AutoCalcFields = False
    Connection = dmADO.AztecConn
    CommandTimeout = 0
    Parameters = <
      item
        Name = 'SiteCode'
        DataType = ftInteger
        Size = -1
        Value = Null
      end>
    Prepared = True
    SQL.Strings = (
      'declare @MatrixID integer'
      
        'select @MatrixID = CurrentMatrix from SiteMatrix where SiteCode ' +
        '= :SiteCode'
      ''
      'select ISNULL(pbn.DisplayName, s.Band) Displayname from'
      
        '     (select distinct band from pbandval where MatrixID = @Matri' +
        'xID) s'
      '  LEFT OUTER JOIN PriceBandNames pbn ON s.Band = pbn.Band'
      '  AND pbn.MatrixID = @MatrixID'
      'where s.Band is not null'
      'order by s.Band'
      '')
    Left = 176
    Top = 152
  end
